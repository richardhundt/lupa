class Point {
   has x : Number = 0
   has y : Number = 0

   our MODE : String = 'cool'

   static has DEBUG : Boolean = false

   static method __call() {
      self.new
   }

   method init() {
      MODE = 'awesome!'
      print("HELLS BELLS, this is ${MODE}", self, Point3D)
   }
   method move(x = 0, y = 0) {
      .x = x
      .y = y
   }
   has deep = { hop = 'jump' }
}

class Point3D from Point {
   has z : Number = 0
   method greet() {
      print("Hello World!")
   }
   method move(x = 0, y = 0, z = 0) {
      super.move(x, y)
      .z = z
   }
}

